//LeetCode Remove Elements Question.
class Solution {
    public int removeElement(int[] n, int val) {
        int ans=0;
        int index=0;
        for(int i=0;i<n.length;i++){
            if(n[i]!=val){
                ans++;
                n[index]=n[i];
                index++;
            }
        }
        return ans;
    }
}
